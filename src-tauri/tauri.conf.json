{
  "productName": "trade_tip",
  "version": "0.0.0",
  "identifier": "com.tauri.dev",
  "build": {
    "beforeDevCommand": "pnpm dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "pnpm build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "label": "main",
        "title": "trade_tip",
        "width": 800,
        "height": 600,
        "decorations": false,
        "resizable": true
      },
      {
        "label": "tool",
        "title": "tool",
        "width": 80,
        "height": 200,
        "resizable": false,
        "alwaysOnTop": true,
        "skipTaskbar": true,
        "decorations": false,
        "visible": false,
        "url": "http://localhost:1420/#/tool"
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
